<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.joo.mybatis.Dao">
  <select id="getAll" resultType="com.joo.mybatis.MDto">
    select * from mybatisdummy
  </select>
  <insert id="insert" parameterType="com.joo.mybatis.MDto">
  	insert into mybatisdummy values(null,#{id},#{pw},#{content})
  </insert>
</mapper>